{
	"name": "express-server",
	"version": "1.0.0",
	"description": "Express.js server with TypeScript",
	"main": "dist/app.js",
	"scripts": {
		"dev": "nodemon src/app.ts",
		"build": "tsc",
		"start": "node dist/app.js",
		"db:generate": "prisma generate",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:unit": "jest --testPathPattern=tests/unit",
		"test:integration": "jest --testPathPattern=tests/integration",
		"test:docker": "docker-compose run --rm test npm run test",
		"test:unit:docker": "docker-compose run --rm test npm run test:unit",
		"test:integration:docker": "docker-compose run --rm test npm run test:integration",
		"docker:up": "docker-compose up --build",
		"docker:down": "docker-compose down",
		"docker:clean": "docker-compose down -v",
		"docker:clean:test": "docker-compose down -v && docker volume rm be_pgdata_test || true"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"@prisma/client": "^6.18.0",
		"@types/cookie-parser": "^1.4.10",
		"@types/socket.io": "^3.0.2",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"bcrypt": "^6.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"dotenv": "^17.2.3",
		"eslint": "^9.39.0",
		"express": "^5.1.0",
		"jsonwebtoken": "^9.0.2",
		"socket.io": "^4.8.1",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"zod": "^4.1.12"
	},
	"devDependencies": {
		"@types/bcrypt": "^6.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/jest": "^30.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/node": "^24.9.2",
		"@types/supertest": "^6.0.3",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"jest": "^30.2.0",
		"nodemon": "^3.1.10",
		"prisma": "^6.18.0",
		"supertest": "^7.1.4",
		"ts-jest": "^29.4.5",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	}
}
